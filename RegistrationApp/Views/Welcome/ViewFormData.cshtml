@model RegistrationModel

@{
    ViewData["Title"] = "Welcome";
}

<h2>Welcome @Model.Name !</h2>

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>
                            Name
                        </th>
                        <th>
                            Address
                        </th>
                        <th>
                            Phone Number
                        </th>
                        <th>
                            Email Address
                        </th>
                        <th>
                            Age
                        </th>
                        <th>
                            Contact Preference
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="table-success">
                        <td>
                            @Model.Name
                        </td>
                        <td>
                            @Model.Address
                        </td>
                        <td>
                            @Model.Phone
                        </td>
                        <td>
                            @Model.Email
                        </td>
                        <td>
                            @Model.Age
                        </td>
                        <td>
                            @Model.ContactPreference
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>